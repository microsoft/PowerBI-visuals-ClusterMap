{
  "name": "ClusterMap",
  "version": "1.2.0",
  "description": "Cluster Map - PowerBI custom visual.",
  "main": "src/index.js",
  "scripts": {
    "preinstall:prereq": "npm install targz mv --save-dev",
    "preinstall:submodules": "node bin/downloadPrivateSubmodules",
    "preinstall:dependencies": "node bin/installSubmoduleDeps",
    "preinstall": "npm run preinstall:prereq && npm run preinstall:submodules && npm run preinstall:dependencies",
    "postinstall": "node bin/createSymLink",
    "install-certificate": "node bin/openCert",
    "start": "npm run dev",
    "tdd": "npm-run-all --parallel dev test:tdd",
    "dev": "node bin/startDev",
    "package": "node bin/packageVisual",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "tslint --project ./tsconfig.json src/**/*.ts",
    "clean": "rm -rf node_modules .tmp yarn.lock"
  },
  "config": {},
  "keywords": [],
  "author": "Microsoft (experimental)",
  "license": "SEE LICENSE IN LICENSE",
  "privacyTerms": "https://privacy.microsoft.com/en-US/privacystatement/",
  "repository": {
    "type": "git",
    "url": "https://github.com/Microsoft/PowerBI-visuals-ClusterMap"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/jquery": "^3.2.17",
    "@types/lodash": "^3.10.1",
    "@types/node": "^9.4.1",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-latest": "^6.24.1",
    "chokidar": "^1.7.0",
    "clean-css": "^4.1.7",
    "connect": "^3.6.2",
    "imports-loader": "^0.7.1",
    "jest": "^22.4.3",
    "jest-junit-reporter": "^1.1.0",
    "jquery": "^3.3.1",
    "json": "^9.0.6",
    "memory-fs": "^0.4.1",
    "mkdirp": "^0.5.1",
    "mv": "^2.1.1",
    "node-sass": "^4.5.3",
    "node-zip": "^1.1.1",
    "npm-run-all": "^4.0.2",
    "onchange": "^3.2.1",
    "powerbi-visuals": "^0.6.0",
    "serve-static": "^1.12.3",
    "string-replace-loader": "^1.3.0",
    "targz": "^1.0.1",
    "ts-jest": "^22.4.4",
    "ts-loader": "^2.3.2",
    "tslint": "^5.8.0",
    "tslint-loader": "^3.5.3",
    "tslint-microsoft-contrib": "^5.0.1",
    "typescript": "^2.6.2",
    "webpack": "^3.4.1"
  },
  "dependencies": {
    "d3": "^4.11.0",
    "matter-attractors": "^0.1.6",
    "matter-js": "^0.13.0",
    "opentype.js": "^0.6.9",
    "powerbi-visuals-utils-formattingutils": "^3.0.1",
    "webcola": "^3.3.8"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,ts}"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "cobertura"
    ],
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "modulePathIgnorePatterns": [
      "lib"
    ],
    "testResultsProcessor": "./node_modules/jest-junit-reporter",
    "transform": {
      "^.+\\.(t|j)sx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "verbose": true
  },
  "privateSubmodules": {
    "@uncharted/personas": "0.7.2"
  }
}
